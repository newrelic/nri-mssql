{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["name", "protocol_version", "integration_version", "data"],
    "properties": {
      "name": {
        "type": "string",
        "description": "Name of the integration"
      },
      "protocol_version": {
        "type": "string",
        "description": "Version of the protocol"
      },
      "integration_version": {
        "type": "string",
        "description": "Version of the integration"
      },
      "data": {
        "type": "array",
        "items": {
          "type": "object",
          "required": ["entity", "metrics", "inventory", "events"],
          "properties": {
            "entity": {
              "type": "object",
              "required": ["name", "type", "id_attributes"],
              "properties": {
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "id_attributes": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "required": ["Key", "Value"],
                    "properties": {
                      "Key": {
                        "type": "string"
                      },
                      "Value": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            },
            "metrics": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "CollectionTimestamp",
                  "database_name",
                  "event_type",
                  "request_start_time",
                  "session_id",
                  "query_text",
                  "reportingEndpoint",
                  "total_wait_time_ms",
                  "wait_category"
                ],
                "properties": {
                  "CollectionTimestamp": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Timestamp when the metrics were collected"
                  },
                  "database_name": {
                    "type": "string",
                    "description": "Name of the database"
                  },
                  "event_type": {
                    "type": "string",
                    "description": "Type of the event"
                  },
                  "request_start_time": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Timestamp when the request started"
                  },
                  "session_id": {
                    "type": "number",
                    "description": "Database session identifier"
                  },
                  "query_text": {
                    "type": "string",
                    "description": "The actual SQL query text"
                  },
                  "reportingEndpoint": {
                    "type": "string",
                    "description": "Endpoint where metrics are reported"
                  },
                  "total_wait_time_ms": {
                    "type": "number",
                    "description": "Total wait time in milliseconds"
                  },
                  "wait_category": {
                    "type": "string",
                    "description": "Category of the wait event"
                  }
                }
              }
            },
            "inventory": {
              "type": "object",
              "description": "Inventory information"
            },
            "events": {
              "type": "array",
              "description": "Array of events"
            }
          }
        }
      }
    }
  }